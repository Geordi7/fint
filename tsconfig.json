{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "es2019",
    "moduleResolution": "node",
  	"declaration": true,
	
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    	
    "outDir": "./dist/"
  },
  "include": ["./src/**/*"],
  "exclude": ["./node_modules/"]
}
