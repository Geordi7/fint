{
  "compilerOptions": {
      "lib": ["esnext"],
      "target": "esnext",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "dist",
      "strict": true,
      "noImplicitAny": true,
      "noFallthroughCasesInSwitch": true,
      "noUnusedParameters": true,
      "declaration": true,
      "sourceMap": true,
      "strictPropertyInitialization": true,
      "strictNullChecks": true,
      "strictFunctionTypes": true
  },
  "include": ["src/**/*"],
}